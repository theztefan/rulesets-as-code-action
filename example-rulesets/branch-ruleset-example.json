{
  "id": 1370416,
  "name": "branch-ruleset-example",
  "target": "branch",
  "source_type": "Organization",
  "source": "theztefan-enterprise-test",
  "enforcement": "disabled",
  "conditions": {
    "repository_name": {
      "exclude": [],
      "include": [
        "important-*"
      ],
      "protected": true
    },
    "ref_name": {
      "exclude": [],
      "include": [
        "~DEFAULT_BRANCH"
      ]
    }
  },
  "rules": [
    {
      "type": "deletion"
    },
    {
      "type": "non_fast_forward"
    },
    {
      "type": "workflows",
      "parameters": {
        "do_not_enforce_on_create": true,
        "workflows": [
          {
            "repository_id": 787291381,
            "path": ".github/workflows/dep-review.yml",
            "ref": "refs/heads/main"
          }
        ]
      }
    },
    {
      "type": "code_scanning",
      "parameters": {
        "code_scanning_tools": [
          {
            "tool": "CodeQL",
            "security_alerts_threshold": "critical",
            "alerts_threshold": "errors_and_warnings"
          }
        ]
      }
    }
  ],
  "bypass_actors": [
    {
      "actor_id": 29110,
      "actor_type": "Integration",
      "bypass_mode": "always"
    },
    {
      "actor_id": 9873994,
      "actor_type": "Team",
      "bypass_mode": "always"
    },
    {
      "actor_id": 1,
      "actor_type": "OrganizationAdmin",
      "bypass_mode": "always"
    }
  ]
}